{
    "version":2,
    
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"static_build"
            }
        }
    
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)/",
            "dest":"poll/wsgi.py"
        }
    ]
}
